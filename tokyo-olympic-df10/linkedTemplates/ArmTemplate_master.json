{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tokyo-olympic-df10"
		},
		"ls_tokyo_olympic_datalakegen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_tokyo_olympic_datalakegen2'"
		},
		"CoachesHttp_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/Coaches.csv"
		},
		"ls_tokyo_olympic_datalakegen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tokyoolympicgen2.dfs.core.windows.net/"
		},
		"ls_tokyo_olympic_github_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/Athletes.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tokyo-olympic-df10_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_tokyo_olympic_datalakegen2_accountKey": {
						"value": "[parameters('ls_tokyo_olympic_datalakegen2_accountKey')]"
					},
					"CoachesHttp_properties_typeProperties_url": {
						"value": "[parameters('CoachesHttp_properties_typeProperties_url')]"
					},
					"ls_tokyo_olympic_datalakegen2_properties_typeProperties_url": {
						"value": "[parameters('ls_tokyo_olympic_datalakegen2_properties_typeProperties_url')]"
					},
					"ls_tokyo_olympic_github_properties_typeProperties_url": {
						"value": "[parameters('ls_tokyo_olympic_github_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}