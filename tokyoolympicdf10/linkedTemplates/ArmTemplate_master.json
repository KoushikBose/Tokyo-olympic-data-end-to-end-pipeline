{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tokyoolympicdf10"
		},
		"ls_athelets_sql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_athelets_sql'"
		},
		"ls_athelets_transformed_gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_athelets_transformed_gen2'"
		},
		"ls_coache_gen2_sql_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_coache_gen2_sql'"
		},
		"ls_coache_sql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_coache_sql'"
		},
		"ls_tokyo_olympic_data_gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_tokyo_olympic_data_gen2'"
		},
		"ls_athelets_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/Athletes.csv"
		},
		"ls_athelets_transformed_gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tokyoolympicgen2.dfs.core.windows.net/"
		},
		"ls_coache_gen2_sql_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tokyoolympicgen2.dfs.core.windows.net/"
		},
		"ls_coaches_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/Coaches.csv"
		},
		"ls_gender_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/EntriesGender.csv"
		},
		"ls_medal_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/Medals.csv"
		},
		"ls_team_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/Medals.csv"
		},
		"ls_tokyo_olympic_data_gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tokyoolympicgen2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "tokyoolympicdf10_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_athelets_sql_connectionString": {
						"value": "[parameters('ls_athelets_sql_connectionString')]"
					},
					"ls_athelets_transformed_gen2_accountKey": {
						"value": "[parameters('ls_athelets_transformed_gen2_accountKey')]"
					},
					"ls_coache_gen2_sql_accountKey": {
						"value": "[parameters('ls_coache_gen2_sql_accountKey')]"
					},
					"ls_coache_sql_connectionString": {
						"value": "[parameters('ls_coache_sql_connectionString')]"
					},
					"ls_tokyo_olympic_data_gen2_accountKey": {
						"value": "[parameters('ls_tokyo_olympic_data_gen2_accountKey')]"
					},
					"ls_athelets_http_properties_typeProperties_url": {
						"value": "[parameters('ls_athelets_http_properties_typeProperties_url')]"
					},
					"ls_athelets_transformed_gen2_properties_typeProperties_url": {
						"value": "[parameters('ls_athelets_transformed_gen2_properties_typeProperties_url')]"
					},
					"ls_coache_gen2_sql_properties_typeProperties_url": {
						"value": "[parameters('ls_coache_gen2_sql_properties_typeProperties_url')]"
					},
					"ls_coaches_http_properties_typeProperties_url": {
						"value": "[parameters('ls_coaches_http_properties_typeProperties_url')]"
					},
					"ls_gender_http_properties_typeProperties_url": {
						"value": "[parameters('ls_gender_http_properties_typeProperties_url')]"
					},
					"ls_medal_http_properties_typeProperties_url": {
						"value": "[parameters('ls_medal_http_properties_typeProperties_url')]"
					},
					"ls_team_http_properties_typeProperties_url": {
						"value": "[parameters('ls_team_http_properties_typeProperties_url')]"
					},
					"ls_tokyo_olympic_data_gen2_properties_typeProperties_url": {
						"value": "[parameters('ls_tokyo_olympic_data_gen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "tokyoolympicdf10_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'tokyoolympicdf10_ArmTemplate_0')]"
			]
		}
	]
}